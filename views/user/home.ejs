

<%- include('../partials/user-header') %>


    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="d-block " src="/images/dairymilk1.jpg" alt="First slide" style="width:100vw;height:75vh;">
        </div>
        <div class="carousel-item">
          <img class="d-block"  src="/images/dairymilk2.jpg" alt="Second slide" style="width:100vw;height:75vh;" >
        </div>
        <div class="carousel-item">
          <img class="d-block"  src="/images/dairymilkminister.jpg" alt="Second slide" style="width:100vw;height:75vh;" >
        </div>
        <div class="carousel-item">
          <img class="d-block"  src="/images/diarybesttheme.jpg" alt="Second slide" style="width:100vw;height:75vh;" >
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/images/dairymilk4.jpg"  style="width:100vw;height:75vh;">
        </div>
      </div>
      <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    <div class="row">
    <div class="middle">
       <h1 class=" text-center p-3 text-white ">Enjoyi!! with us</h1>
    </div>
  </div>
    <div class="container">
      <div class="row">
        <% product.forEach((products)=>{ %>
        <div class="col-12  col-md-3">
          <div class="card m-4" style="width: 15rem;">
            <img class="card-img-top " src="/admin-images/<%=products?._id%>.jpg" style="width:10rem;height:10rem;margin: auto;" alt="Card image cap">
            <div class="card-body">
              <h5 class="card-title"><%= products?.name %></h5>
              <p class="card-text"><%= products?.category %></p>
          
           <h4 class="card-text text-danger">â‚¹<%= products.price %></h4>
           <% if(customer){ %>
              <a href="/product-view/<%= products?._id %>" class="btn btn-primary">Buy now</a>
              <% }else{ %>
                <a href="/login" class="btn btn-danger">Buy now</a>
                <% } %>
            </div>
          </div>
        </div>
     <% }) %>
    </div>
  </div>
     
